import time
from collections import deque


class FrequencyPoint:

    def __init__(self,timelist: list):
        self.frequencies=[1./(t2-t1)
                          for t2,t1 in zip(timelist[1:],timelist)]
        self.

class FrequencyMonitoring:

    def __init__(self,maxlen:int):
        self._queue=deque([None],maxlen=maxlen)
        self._maxlen=maxlen
        
    def ping(self):
        self._queue.append(time.time())


        